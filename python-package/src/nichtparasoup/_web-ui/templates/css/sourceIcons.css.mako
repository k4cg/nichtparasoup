#wall figure figcaption {
    /* FALLBACK SOURCE: https://commons.wikimedia.org/wiki/File:Question_Mark_Icon_-_Blue_Box.svg */
    background-image: url('data:image/png;base64,');
}

% for crawler_name, crawler_icon_url in names_icons_list:
    #wall figure figcaption[data-crawler='${crawler_name.replace("'", "")}'] {
        background-image: url('${crawler_icon_url.replace("'", "")}');
    }
% endfor
